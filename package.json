{
  "name": "odbm-next16-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.x",
    "pnpm": "10.x"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:standalone": "pnpm build && node scripts/copy-static-to-standalone.js && node scripts/lift-pnpm-standalone.js",
    "start": "next start",
    "start:standalone": "node .next/standalone/server.js",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "check-types": "tsc --noEmit",
    "bump-deps-minor": "ncu --target minor -u && pnpm install",
    "bump-deps": "ncu -x recharts -u && pnpm install",
    "bump-ui": "npx shadcn@latest add --all --overwrite",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@microsoft/applicationinsights-web": "^3.3.10",
